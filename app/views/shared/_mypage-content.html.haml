          =link_to "MUJI Card会員情報とMUJI.netメンバー情報をつなぐ","#", class: "mypage-content--main--MUJI-CARD--list--connect--link"
        %li.mypage-content--main--MUJI-CARD--list--no-member
          =link_to "マイル・ショッピングポイント確認会員情報を繋いでない方", "#", class: "mypage-content--main--MUJI-CARD--list--no-member--link"
        %li.mypage-content--main--MUJI-CARD--list--guide
          =link_to "MIJI Cardのご案内", "#", class: "mypage-content--main--MUJI-CARD--list--guide--link"
    %li.mypage-content--main--else
      .mypage-content--main--else--title
        %h2.mypage-content--main--else--title--text その他
      %ul.mypage-content--main--else--list
        %li.mypage-content--main--else--list--camp
          =link_to "キャンプ場ご予約確認・変更", "#", class: "mypage-content--main--else--list--camp--link"
  = link_to "退会の手続き", "#", class: "mypage-content--unsubscribe"
